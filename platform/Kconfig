config PLATFORM
	string
	default "sun50i" if PLATFORM_SUN50I

choice
	prompt "Platform selection"

config PLATFORM_SUN50I
	bool "sun50i (A64/H5)"

endchoice

if PLATFORM_SUN50I

source "sun50i/Kconfig"

endif
